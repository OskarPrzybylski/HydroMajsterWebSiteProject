@model HydroMajsterWebShopProject.Views.ViewModels.CategoryViewModel

<h2>AddCategory</h2>
<form method="post" enctype="multipart/form-data" asp-controller="AdminPanel" asp-action="AddCategory">
    <div class="form-group">
        <div class="col-md-10">
            <p>Zdjęcie produktu:</p>
            <input type="file" name="file" />
        </div>
    </div>
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" />
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Dodaj kategorie" />
        </div>
    </div>
</form>

